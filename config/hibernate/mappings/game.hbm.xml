<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.scorebook.parser.java.Game" table="Game">
		<id name="id" type="int" column="ID">
			<generator class="assigned" />
		</id>
	</class>

	<property name="status">
		<column name="status" />
	</property>

	<property name="home_score">
		<column name="home_score"/>
	</property>

	<property name="away">
		<column name="Away" />
	</property>

	<property name="home">
		<column name="Home" />
	</property>
	
</hibernate-mapping>